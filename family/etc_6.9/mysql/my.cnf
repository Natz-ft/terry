[mysqld_multi]
mysqld      =   /home/libo/local/mysql/bin/mysqld_safe
mysqladmin  =   /home/libo/local/mysql/bin/mysqladmin
log        =    /home/mysql/mysql-5.7.18/logs/mysqld_multi.log
user        =   mysql
password    =   mysql

[mysqld3306]
character-set-server=utf8
port=3306

basedir=/home/libo/local/mysql
datadir=/home/mysql/mysql-5.7.18/databases/data3306 #数据库目录
socket=/home/mysql/mysql-5.7.18/logs/mysqld3306.sock  #socket连接存根
log-error=/home/mysql/mysql-5.7.18/logs/error-mysqld3306.log #错误日志
pid-file=/home/mysql/mysql-5.7.18/logs/mysqld3306.pid  #进程文件
language=/home/libo/local/mysql/share/english       #使用提示语言，这里目录下必须包含errmsg.sys

max_allowed_packet = 128M
query_cache_size=128M
max_connections=20
max_connect_errors=100
key_buffer_size=60M
read_buffer_size=32M
read_rnd_buffer_size = 32M
myisam_sort_buffer_size=512M
tmp_table_size=256M
sort_buffer_size=128M

symbolic-links=0

